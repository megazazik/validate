{
	"compilerOptions": {
		"sourceMap": false,
		"module": "commonjs",
		"target": "es2017",
		"declaration": true,
		"moduleResolution": "node",
		"outDir": "./dist/",
		"lib": ["es2015", "es5"],
		"esModuleInterop": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true
	}
}
